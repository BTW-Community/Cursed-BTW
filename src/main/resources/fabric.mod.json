{
  "schemaVersion": 1,
  "id": "cursedbtw",
  "version": "${version}",

  "name": "BTW Fabric Mod",
  "description": "This is the BTW Fabric port!",
  "authors": [
    "BTW Community"
  ],
  "contact": {
    "homepage": "https://www.sargunster.com/btwforum/",
    "sources": "https://https://github.com/BTW-Community/Cursed-BTW"
  },

  "license": "CC-BY-4.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "btw.community.fabric.BTWFabricMod"
    ],
    "gfh:prePreLaunch": [
      "btw.community.fabric.BTWFabricMod"
    ]
  },
  "mixins": [
    "cursedbtw.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  }
