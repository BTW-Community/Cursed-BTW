{
  "schemaVersion": 1,
  "id": "btw",
  "name": "Better Than Wolves CE",
  "version": "3.0.0 Beta Snapshot 3",
  "environment": "*",
  "description": "This is a continuation of FlowerChild's Better than Wolves total conversion Minecraft mod.",
  "contact": {
    "issues": "https://github.com/BTW-Community/BTW-Public/issues",
    "sources": "https://github.com/BTW-Community/BTW-Public"
  },
  "authors": [
    "FlowerChild",
    "Better than Wolves Community"
  ],
  "license": "Creative Commons Attribution 4.0 International",
  "icon": "btw_logo.png",
  "entrypoints": {
    "init": [
      "btw.community.fabric.BTWFabricMod"
    ],
    "gfh:prePreLaunch": [
      "btw.community.fabric.BTWFabricMod"
    ],
    "btw:addon": [
      "btw.BTWMod",
      "emi.dev.emi.emi.EMIPostInit"
    ],
    "emi": [
      "emi.dev.emi.emi.api.plugin.VanillaPlugin",
      "emi.dev.emi.emi.api.plugin.BTWPlugin"
    ]
  },
  "mixins": [
    "cursedbtw.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "btw:addon_prefix": "BTW"
  }
}